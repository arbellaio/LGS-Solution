            <!-- Profile Image -->
            <div class="box box-primary">
                <div class="box-body box-profile">
                    @if (string.IsNullOrEmpty(Model.UserVm.Client.ProfilePhoto))
                    {
                        <img type="file" class="profile-user-img img-responsive img-circle" style="cursor: pointer" id="DetailProfilePic" src="../../LgsImageRepo/ProfileImages/profiledefault.png" alt="User profile picture">
                        @*                 <input type="file" id="my_file" name="ProfilePic" disabled style="display: none;" /> *@

                    }
                    else
                    {
                        var absolutePath = HttpContext.Current.Server.MapPath("~/" + Model.UserVm.Client.ProfilePhoto);
                        if (!@System.IO.File.Exists(absolutePath))
                        {
                            <img type="file" class="profile-user-img img-responsive img-circle" style="cursor: pointer" id="DetailProfilePic" src="../../LgsImageRepo/ProfileImages/profiledefault.png" alt="User profile picture">
                        }
                        else
                        {
                            <img type="file" class="profile-user-img img-responsive img-circle" style="cursor: pointer" id="DetailProfilePic" src="@Model.UserVm.Client.ProfilePhoto" alt="User profile picture">
                        }
